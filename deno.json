{
  "fmt": {
    "lineWidth": 120
  },
  "imports": {
    "cliffy/": "https://deno.land/x/cliffy@v1.0.0-rc.1/",
    "zod/": "https://deno.land/x/zod@v3.21.4/",
    "puppeteer/": "https://deno.land/x/puppeteer@16.2.0/",
    "std/": "https://deno.land/std@0.192.0/",
    "ms": "npm:ms@^2",
    "faker": "npm:@faker-js/faker@^8",
    "radash": "npm:radash@^10",
    "cosmiconfig": "npm:cosmiconfig@^8"
  },
  "tasks": {
    "build": "deno compile --allow-env --allow-write --allow-read --allow-run --allow-net -o bin/chaos oa.ts",
    "dev": "deno run --allow-env --allow-write --allow-read --allow-run --allow-net --watch oa.ts",
    "install": "deno install -f --allow-env --allow-write --allow-read --allow-run --allow-net oa.ts"
  }
}
